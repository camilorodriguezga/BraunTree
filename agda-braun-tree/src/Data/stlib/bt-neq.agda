module Data.stlib.bt-neq where

open import Data.stlib.bt-eq
open import Data.stlib.bt-negation

----------------------------------------------------------------------
-- syntax
----------------------------------------------------------------------

infix 4 _≢_ 

----------------------------------------------------------------------
-- defined types
----------------------------------------------------------------------

_≢_ : ∀ {ℓ}{A : Set ℓ} → A → A → Set ℓ
x ≢ y = ¬ (x ≡ y)
